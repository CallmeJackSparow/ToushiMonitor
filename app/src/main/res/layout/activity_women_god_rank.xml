                android:layout_width="142px"
                android:layout_height="142px"
                android:background="@drawable/rank1"
                android:layout_centerHorizontal="true"/>

            <ImageView
                android:id="@+id/womenRank1Image"
                android:layout_below="@+id/rankTitleBack"
                android:layout_marginTop="40px"
                android:layout_width="100px"
                android:layout_height="100px"
                android:layout_centerHorizontal="true"
                android:background="@drawable/circle"/>

            <TextView
                android:id="@+id/womanRank1ScoreLabel"
                android:layout_below="@+id/rankTitleBack"
                android:layout_marginTop="150px"
                android:layout_width="100px"
                android:layout_height="100px"
                android:layout_marginLeft="370px"
                android:text="100"
                android:textSize="20dp"
                android:textColor="#FFFFFF"
                android:fontFamily="@font/sourcehansans"/>

            <ImageView
                android:id="@+id/womenRank2"
                android:layout_below="@+id/rankTitleBack"
                android:layout_marginTop="120px"
                android:layout_width="104px"
                android:layout_height="104px"
                android:background="@drawable/rank2"
                android:layout_marginLeft="200px" />

            <ImageView
                android:id="@+id/womenRank2Image"
                android:layout_below="@+id/rankTitleBack"
                android:layout_marginTop="130px"
                android:layout_width="85px"
                android:layout_height="85px"
                android:layout_marginLeft="210px"
