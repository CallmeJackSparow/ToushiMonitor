        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="322px">

            <ImageView
                android:layout_marginLeft="200px"
                android:layout_width="48px"
                android:layout_height="322px"
                android:background="@drawable/link_left"/>

            <ImageView
                android:layout_marginLeft="832px"
                android:layout_width="48px"
                android:layout_height="322px"
                android:background="@drawable/link_right"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="826px"
            android:layout_height="1496px"
            android:background="@drawable/popupwindow"
            android:layout_gravity="center">


            <ImageView
                android:id="@+id/rankTitleBack"
                android:layout_width="560px"
                android:layout_height="120px"
                android:layout_marginTop="20px"
                android:background="@drawable/ranktitle"
                android:layout_marginLeft="60px"/>

            <TextView
                android:layout_width="560px"
                android:layout_height="120px"
                android:layout_marginTop="0px"
                android:text="女神榜"
                android:textColor="#FFFF"
                android:textSize="60px"
                android:textAlignment="center"
                android:layout_centerHorizontal="true"
                android:fontFamily="@font/sourcehansans"/>

            <ImageView
                android:id="@+id/rostram"
                android:layout_below="@+id/rankTitleBack"
                android:layout_marginTop="142px"
                android:layout_width="400px"
                android:layout_height="158px"
                android:background="@drawable/rankbackground"
                android:layout_centerHorizontal="true"/>

            <ImageView
