            <ImageView
                android:id="@+id/reward4"
                android:layout_width="110px"
                android:layout_height="110px"
                android:layout_below="@id/reward3"
                android:layout_marginTop="10px"
                android:layout_marginLeft="50px"
                android:background="@drawable/rankimage"/>
            <ImageView
                android:id="@+id/womenReward4head"
                android:layout_gravity="center"
                android:layout_below="@id/reward3"
                android:layout_marginLeft="65px"
                android:layout_width="80px"
                android:layout_height="80px"
                android:layout_marginTop="20px"
                android:background="@drawable/circle"/>

            <ImageView
                android:id="@+id/reward5Image"
                android:layout_width="628px"
                android:layout_height="70px"
                android:layout_below="@id/reward4Image"
                android:layout_marginTop="50px"
                android:layout_marginLeft="120px"
                android:background="@drawable/rankbar" />
            <ImageView
                android:id="@+id/womanReward5ProcessBar"
                android:layout_width="628px"
                android:layout_height="70px"
                android:layout_below="@id/reward4Image"
                android:layout_marginTop="50px"
                android:layout_marginLeft="120px"
                android:background="@drawable/processbar" />
            <TextView
                android:id="@+id/womanRank5Score"
                android:layout_width="100px"
                android:layout_height="80px"
                android:layout_alignTop="@id/womanReward5ProcessBar"
                android:layout_marginTop="-20px"
                android:layout_alignRight="@id/womanReward5ProcessBar"
                android:textColor="#FFFFFF"
                android:text=""
